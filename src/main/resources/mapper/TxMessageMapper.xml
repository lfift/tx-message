        <result column="NEXT_SCHEDULE_TIME" property="nextScheduleTime" />
        <result column="INIT_BACKOFF" property="initBackoff" />
        <result column="BACKOFF_FACTOR" property="backoffFactor" />
        <result column="MESSAGE_STATUS" property="messageStatus" />
        <result column="DELETED" property="deleted" />
